{
  "name": "remix",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "lint": "eslint ./app",
    "build": "remix build",
    "dev": "remix dev --manual",
    "start": "remix-serve ./build/index.js",
    "typecheck": "tsc",
    "prepare": "husky install",
    "prettier": "npx prettier -w ./app",
    "precommit": "npm run lint && npm run prettier",
    "fix": "eslint --fix './app/**/*.ts'"
  },
  "dependencies": {
    "@remix-run/css-bundle": "^2.5.0",
