    "husky": "^8.0.3",
    "inline-fixtures": "^1.1.0",
    "js-green-licenses": "^4.0.0",
    "mocha": "^10.0.0",
    "prettier": "^3.1.0",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "sinon": "^17.0.0",
    "tmp": "0.2.1",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
