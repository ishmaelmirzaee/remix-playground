    "@typescript-eslint/eslint-plugin": "6.18.1",
    "@typescript-eslint/parser": "6.18.1",
    "chalk": "^5.3.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "execa": "^8.0.1",
    "inquirer": "^9.2.12",
    "isbot": "^4.4.0",
    "json5": "^2.2.3",
    "meow": "^13.1.0",
    "ncp": "^2.0.0",
    "prettier": "3.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "5.0.5",
    "write-file-atomic": "^5.0.1"
  },
  "devDependencies": {
