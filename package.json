    "@remix-run/dev": "^2.3.1",
    "@remix-run/eslint-config": "^2.3.1",
    "@types/cross-spawn": "^6.0.2",
    "@types/eslint": "^8.0.0",
    "@types/fs-extra": "^11.0.0",
    "@types/inquirer": "^8.0.0",
    "@types/json5": "0.0.30",
    "@types/mocha": "^10.0.0",
    "@types/ncp": "^2.0.4",
    "@types/node": "20.8.9",
    "@types/prettier": "^2.1.1",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/rimraf": "^3.0.0",
    "@types/sinon": "^10.0.0",
    "@types/tmp": "^0.2.0",
    "@types/write-file-atomic": "^4.0.0",
    "c8": "^8.0.0",
