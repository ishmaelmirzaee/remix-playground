    "@remix-run/eslint-config": "^2.5.0",
    "@types/cross-spawn": "^6.0.6",
    "@types/eslint": "^8.56.2",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/json5": "0.0.30",
    "@types/mocha": "^10.0.6",
    "@types/ncp": "^2.0.8",
    "@types/node": "20.11.0",
    "@types/prettier": "^2.7.3",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@types/rimraf": "^3.0.2",
    "@types/sinon": "^17.0.3",
    "@types/tmp": "^0.2.6",
    "@types/write-file-atomic": "^4.0.3",
