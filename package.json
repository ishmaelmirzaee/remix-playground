    "js-green-licenses": "^4.0.0",
    "mocha": "^10.2.0",
    "prettier": "^3.2.2",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "sinon": "^17.0.1",
    "tmp": "0.2.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
