    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "chalk": "^4.1.2",
    "eslint": "8.54.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "5.0.1",
    "execa": "^5.0.0",
    "inquirer": "^7.3.3",
    "isbot": "^3.6.8",
    "json5": "^2.1.3",
    "meow": "^9.0.0",
    "ncp": "^2.0.0",
    "prettier": "3.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "3.0.2",
    "write-file-atomic": "^4.0.0"
  },
  "devDependencies": {
